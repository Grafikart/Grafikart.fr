{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": "last 2 versions, not dead, not ie 6-11",
        "modules": false,
        "useBuiltIns": false,
        "corejs": null
      }
    ]
  ],
  "plugins": [
    "@babel/plugin-syntax-dynamic-import"
  ],
  "env": {
    "test": {
      "presets": [["@babel/preset-env"]]
    }
  }
}
