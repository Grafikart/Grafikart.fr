    "preact": "^10.4.1",
    "scriptjs": "^2.5.9",
    "sortablejs": "^1.10.2",
    "turbolinks": "^5.2.0"
  },
  "eslintConfig": {
    "extends": "preact",
    "rules": {
      "react/jsx-fragments": 0,
      "import/no-absolute-path": 0
    },
    "env": {
      "browser": true
    }
  },
  "jest": {
    "moduleNameMapper": {
      "/functions/(.*)$": "<rootDir>/../../assets/functions/$1",
      "/elements/(.*)$": "<rootDir>/../../assets/elements/$1"
    },
    "moduleDirectories": [
      "node_modules",
      "bower_components",
      "assets"
    ],
    "rootDir": "tests/js"
  }
