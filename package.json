{
  "name": "grafikart",
  "version": "1.0.91",
  "main": "index.js",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-transform-react-jsx": "^7.12.12",
    "@babel/preset-env": "^7.12.11",
    "@prefresh/vite": "^2.0.0",
    "@types/jest": "^26.0.23",
    "@types/youtube": "^0.0.41",
    "babel-plugin-jsx-pragmatic": "^1.0.2",
    "doctoc": "^2.0.1",
    "eslint-config-preact": "^1.1.3",
    "jest": "^27.0.4",
    "prettier": "^2.3.1",
    "prettier-standard": "^16.4.1",
    "sass": "^1.32.4",
    "vite": "2.3.7"
  },
  "scripts": {
    "lint": "prettier-standard --lint \"assets/**/*.{js,css,jsx}\"",
    "lint-staged": "prettier-standard --lint --changed \"assets/**/*.{js,css,jsx}\"",
    "dev": "rm -rf public/assets/time && npx vite --host=0.0.0.0",
    "build": "npx vite build && touch public/assets/time && find ./public/assets -name \"*.*\" -mtime +7 -delete",
    "test": "jest"
  },
  "dependencies": {
    "@grafikart/spinning-dots-element": "^1.1.0",
    "@preact/signals": "^1.1.3",
    "@types/chart.js": "^2.9.29",
    "body-scroll-lock": "^3.1.5",
    "canvas-confetti": "^1.3.2",
    "chart.js": "^2.9.4",
    "clsx": "^1.2.1",
    "codemirror": "^5.59.0",
    "favicon-badge": "^2.0.0",
    "flatpickr": "^4.6.9",
    "headless-elements": "^0.0.2",
    "htm": "^3.0.4",
    "markdown-to-jsx": "7.1.2",
    "preact": "^10.5.10",
    "react-diff-viewer": "^3.1.1",
    "scriptjs": "^2.5.9",
    "sortablejs": "^1.10.2",
    "tom-select": "^1.7.5",
    "turbolinks": "^5.2.0"
  },
  "eslintConfig": {
    "extends": "preact",
    "rules": {
      "react/jsx-fragments": 0,
      "import/no-absolute-path": 0
    },
    "env": {
      "browser": true
    }
  },
  "jest": {
    "moduleDirectories": [
      "node_modules",
      "assets"
    ],
    "moduleNameMapper": {
      "^/functions/(.*)$": "<rootDir>/../../assets/functions/$1",
      "^/elements/(.*)$": "<rootDir>/../../assets/elements/$1"
    },
    "rootDir": "tests/js"
  }
}
